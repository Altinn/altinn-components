{
  "name": "@altinn/altinn-components",
  "version": "0.56.19",
  "main": "dist/index.js",
  "files": ["dist/", "!dist/stories/", "!dist/components/*/*.stories.js"],
  "types": "dist/types/lib/index.d.ts",
  "type": "module",
  "description": "Reusable react components",
  "sideEffects": ["*.css"],
  "scripts": {
    "test": "vitest run",
    "test:unit": "vitest run --project=unit",
    "storybook": "storybook dev -p 6006",
    "typecheck": "tsc --noEmit",
    "build": "tsc && vite build",
    "format": "lint-staged",
    "lint": "biome check",
    "lint:fix": "biome check ./ --write",
    "dev": "vite",
    "build-storybook": "storybook build",
    "ci:build-storybook": "storybook build --quiet",
    "test-storybook": "vitest --run --project=storybook",
    "test-storybook-watch": "vitest --project=storybook",
    "ci:test-storybook": "pnpm dlx concurrently -k -s first \"pnpm storybook\" \"pnpm dlx wait-on http://localhost:6006 && pnpm test-storybook\"",
    "install:browsers": "pnpm exec playwright install",
    "build-tokens": "node --import tsx scripts/build-tokens.ts && pnpm biome check ./lib/css/tokens/design-tokens-build/. --write"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@digdir/designsystemet-css": "^1.9.0",
    "@digdir/designsystemet-react": "^1.9.0",
    "@digdir/designsystemet-theme": "^1.9.0",
    "@navikt/aksel-icons": "^7.37.0",
    "@tanstack/react-virtual": "^3.13.9",
    "classnames": "^2.5.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@chromatic-com/storybook": "4.1.3",
    "@storybook/addon-a11y": "10.1.11",
    "@storybook/addon-docs": "10.1.11",
    "@storybook/addon-themes": "10.1.11",
    "@storybook/addon-vitest": "^10.1.11",
    "@storybook/react-vite": "10.1.11",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^25.0.7",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "@vitest/browser": "4.0.16",
    "@vitest/browser-playwright": "4.0.16",
    "@vitest/coverage-v8": "^4.0.0",
    "axe-core": "^4.10.3",
    "axe-playwright": "^2.1.0",
    "glob": "^13.0.0",
    "lint-staged": "^15.5.2",
    "playwright": "^1.57.0",
    "rollup-preserve-directives": "^1.1.3",
    "storybook": "^10.1.11",
    "storybook-addon-tag-badges": "^3.0.4",
    "storybook-addon-theme-provider": "^0.2.10",
    "tsx": "^4.21.0",
    "typescript": "^5.8.3",
    "vite": "7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-lib-inject-css": "^2.2.2",
    "vite-plugin-static-copy": "^2.3.2",
    "vitest": "4.0.16"
  },
  "peerDependencies": {
    "react": ">=18.3.1 || ^19.0.0",
    "react-dom": ">=18.3.1 || ^19.0.0"
  },
  "lint-staged": {
    "**/*.*": "biome check --staged --no-errors-on-unmatched --write"
  },
  "packageManager": "pnpm@9.13.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Altinn/altinn-components.git"
  }
}
